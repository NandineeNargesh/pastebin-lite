generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
 
}

model Paste {
  id            String   @id @default(cuid())
  title         String?  // Add this line if it's missing
  content       String
  current_views Int      @default(0)
  max_views     Int?
  expires_at    DateTime?
  created_at    DateTime @default(now())
}